<p id="notice"><%= notice %></p>
<%= link_to "back" %>
<h1><center>List Students</center></h1>
<center>
<table class="table-bordered">
  <thead>
    <tr class="title_table" style="width:100%">
      <th style="width:100px">ID</th>
      <th style="width:200px">Name</th>
      <th style="width:250px">Email</th>
      <th style="width:100px">Avatar</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
    <tr class="content_table">
      <td style="height:60px"><%= student.id %></td>
      <td style="height:60px"><%= student.name %></td>
      <td style="height:60px"><%= student.email %></td>
      <td style="height:60px"><% if current_student.avatar.attached? %>
        <%= image_tag student.avatar_thumbnail,class:"imageee"%>
        <% else %>
        <div class="acc2"><svg viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" style="display: block; height: 40px; width: 40px; fill: currentcolor;" aria-hidden="true" role="presentation" focusable="false">
          <path d="m16 .7c-8.437 0-15.3 6.863-15.3 15.3s6.863 15.3 15.3 15.3 15.3-6.863 15.3-15.3-6.863-15.3-15.3-15.3zm0 28c-4.021 0-7.605-1.884-9.933-4.81a12.425 12.425 0 0 1 6.451-4.4 6.507 6.507 0 0 1 -3.018-5.49c0-3.584 2.916-6.5 6.5-6.5s6.5 2.916 6.5 6.5a6.513 6.513 0 0 1 -3.019 5.491 12.42 12.42 0 0 1 6.452 4.4c-2.328 2.925-5.912 4.809-9.933 4.809z"></path></svg>
        </div>
        <% end %>
      </td>
  </tr>
  <% end %>
</tbody>
</table>
</center>
