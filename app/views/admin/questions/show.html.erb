<%= link_to '<i class="far fa-edit"></i>'.html_safe,edit_admin_question_path()  ,class: "btn btn-secondary"%>
<%= link_to '<i class="fas fa-arrow-circle-left"></i>'.html_safe, admin_questions_path(),class: "btn btn-primary" %>
<%= link_to '<i class="fas fa-trash-alt"></i>'.html_safe, admin_questions_path(),class: "btn btn-info" %>

<div class="col8">
  <div class="pbp">Questions (<%=@course.question.count%>)</div>
  <% @course.questions.each.with_index(1) do |question, index|%>
  <div class="pb">
    <div class=""><%=index%>- Quiz</div>
    <div class="fXyePK"><%= question.title %></div>
  </div>
  <%end%>
</div>
